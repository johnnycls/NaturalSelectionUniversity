	else:
		do Game.hospital.buy_blood(false)
	\doctor: HOSPITAL_7
- HOSPITAL_4
	\doctor: HOSPITAL_6
	do Game.hospital.sell_blood()
	\doctor: HOSPITAL_7
	jump choice
- HOSPITAL_5
\doctor: HOSPITAL_9
